#pragma config(Motor,  port3,           Motor2,        tmotorServoContinuousRotation, openLoop)
#pragma config(Motor,  port6,           leftMotor,     tmotorServoContinuousRotation, openLoop)
#pragma config(Motor,  port7,           Motor1,        tmotorServoContinuousRotation, openLoop)
#pragma config(Motor,  port8,           serv1,         tmotorServoStandard, openLoop)
#pragma config(Motor,  port9,           serv2,         tmotorServoStandard, openLoop)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//
int motorvar;
int butvar;
int motorsp;
void butspeed(){
		motor[motorvar]=vexRT[butvar]*motorsp;}
task main(){
int motorspeed;
int motorspeed1;
	while(true){
		motor[leftMotor]= vexRT[Ch2];
		motor[rightMotor]= vexRT[Ch3];
		if(vexRT[Btn8D]==1){
			motorspeed=50;}
		if(vexRT[Btn8U]==1){
			motorspeed=100;}
		if(vexRT[Btn8R]==1){
			motorspeed=50;}
		if(vexRT[Btn8L]==1){
				motorspeed=100;}
			motorvar=Motor1;
			butvar=Btn5D;
			motorsp=motorspeed;
			butspeed();
		motor[Motor1]=vexRT[Btn5D]*motorspeed;
		motor[Motor2]=vexRT[Btn5U]*motorspeed;
		motor[Motor1]=vexRT[Btn6D]*-motorspeed;
		motor[Motor2]=vexRT[Btn6U]*-motorspeed;
		motor[serv1]=vexRT[Btn7R]*-motorspeed1;
		motor[serv2]=vexRT[Btn7L]*motorspeed1;
		motor[serv1]=vexRT[Btn7R]*-motorspeed1;
		motor[serv2]=vexRT[Btn7L]*motorspeed1;}}
